	<arg:Model class="ORMAbstractionToConceptualDatabaseBridgeDomainModel">
		<arg:RuleContainer class="ORMAbstractionToConceptualDatabaseBridgeDomainModel.ModificationTracker">
			<arg:ChangeRule methodName="AbstractionModelChangedRule">
				<arg:RuleOn targetType="AbstractionModel" targetTypeQualifier="Neumont.Tools.ORMAbstraction"/>
			</arg:ChangeRule>
			<arg:AddRule methodName="ConceptTypeAddedRule">
				<arg:RuleOn targetType="AbstractionModelHasConceptType" targetTypeQualifier="Neumont.Tools.ORMAbstraction"/>
			</arg:AddRule>
			<arg:ChangeRule methodName="ConceptTypeChildChangedRule">
				<arg:RuleOn targetType="ConceptTypeChild" targetTypeQualifier="Neumont.Tools.ORMAbstraction"/>
			</arg:ChangeRule>
			<arg:DeleteRule methodName="ConceptTypeDeletedRule">
				<arg:RuleOn targetType="AbstractionModelHasConceptType" targetTypeQualifier="Neumont.Tools.ORMAbstraction"/>
			</arg:DeleteRule>
			<arg:AddRule methodName="InformationTypeFormatAddedRule">
				<arg:RuleOn targetType="AbstractionModelHasInformationTypeFormat" targetTypeQualifier="Neumont.Tools.ORMAbstraction"/>
			</arg:AddRule>
			<arg:DeleteRule methodName="InformationTypeFormatDeletedRule">
				<arg:RuleOn targetType="AbstractionModelHasInformationTypeFormat" targetTypeQualifier="Neumont.Tools.ORMAbstraction"/>
			</arg:DeleteRule>
			<arg:DeleteRule methodName="UniquenessDeletedRule">
				<arg:RuleOn targetType="UniquenessConstraintIsForUniqueness"/>
			</arg:DeleteRule>
		</arg:RuleContainer>
		<arg:RuleContainer class="AssimilationMapping">
			<!-- UNDONE: AssimilationMappingKeepAlive, temporary rule until the assimilations are not lost during remapping -->
			<arg:AddRule methodName="FactTypeAddedToPathRule">
				<arg:RuleOn targetType="ConceptTypeChildHasPathFactType" targetTypeQualifier="Neumont.Tools.ORMToORMAbstractionBridge"/>
			</arg:AddRule>
			<!-- UNDONE: AssimilationMappingKeepAlive, temporary rule until the assimilations are not lost during remapping -->
			<arg:AddRule methodName="AssimilationMappingAddedRule">
				<arg:RuleOn targetType="AssimilationMappingCustomizesAssimilation"/>
			</arg:AddRule>
		</arg:RuleContainer>
	</arg:Model>
</arg:Rules>
